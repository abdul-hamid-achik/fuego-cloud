[master]
master ansible_host=${master_ip} ansible_user=root private_ip=${master_private}

[workers]
%{ for i, ip in worker_ips ~}
worker-${i + 1} ansible_host=${ip} ansible_user=root private_ip=${worker_privates[i]}
%{ endfor ~}

[k3s_cluster:children]
master
workers

[k3s_cluster:vars]
ansible_ssh_common_args='-o StrictHostKeyChecking=no'
